{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "module": "esnext",
    "lib": ["esnext", "DOM"],
    "types": ["@cloudflare/workers-types"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "jsxImportSource": "hono/jsx"
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
